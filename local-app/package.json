{
  "name": "notionally-local-app",
  "version": "1.8.0",
  "description": "Local processing app for Notionally - LinkedIn to Notion video saver",
  "main": "src/server.js",
  "scripts": {
    "start": "./scripts/start.sh",
    "start:direct": "node src/server.js",
    "start:original": "node src/server-original.js",
    "dev": "node scripts/clear-port.js && node src/start-with-setup.js",
    "dev:simple": "node scripts/clear-port.js && nodemon src/server.js",
    "dev:skip-setup": "node scripts/clear-port.js && node src/start-with-setup.js --skip-setup",
    "setup": "node src/setup/interactive-setup.js",
    "setup:dropbox": "node scripts/get-dropbox-token.js",
    "check": "node scripts/check-setup.js",
    "clear-port": "node scripts/clear-port.js",
    "analyze-investigation": "node scripts/analyze-investigation.js",
    "analyze-telemetry": "node scripts/analyze-telemetry.js",
    "check-versions": "node scripts/check-versions.js",
    "update-version": "node scripts/check-versions.js --update",
    "test-entities": "node scripts/test-html-entities.js",
    "test": "npm run test-entities",
    "fetch-data-source-id": "node scripts/fetch-data-source-id.js"
  },
  "keywords": [
    "linkedin",
    "notion",
    "video",
    "scraping",
    "dropbox"
  ],
  "author": "Fredrik Matheson",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^5.1.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "dropbox": "^10.34.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.9.4",
    "path": "^0.12.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
